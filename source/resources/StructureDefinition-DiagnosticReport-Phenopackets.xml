<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <url value="http://ga4gh.org/fhir/phenopackets/StructureDefinition/DiagnosticReport-Phenopackets" />
  <name value="DiagnosticReport-Phenopackets" />
  <title value="Diagnostic Report - Phenopackets" />
  <status value="draft" />
  <description value="Represents the test results of an individual." />
  <fhirVersion value="4.0.0" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="DiagnosticReport" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/DiagnosticReport" />
  <derivation value="constraint" />
  <differential>
    
    <!-- Root node - required for differential -->
    <element id="DiagnosticReport">
      <path value="DiagnosticReport" />
    </element>
    
    <!-- Set up slicing -->
    <element id="DiagnosticReport.extension">
      <path value="DiagnosticReport.extension" />
      <!-- Slice the extension element -->
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <!-- We don't care what the order of any extensions is -->
        <ordered value="false"/>
        <rules value="open" />
      </slicing>
    </element>
    
    <!-- The slice that contains the variants -->
    <element id="DiagnosticReport.extension:variants">
      <path value="DiagnosticReport.extension" />
      <sliceName value="variants" />
      <sliceIsConstraining value="false" />
      <label value="variants" />
      <short value="List of variants determined to be present in the biosample." />
      <definition value="List of variants determined to be present in the biosample." />
      <min value="0" />
      <max value="*" />
      <type>
        <code value="Extension" />
        <profile value="http://hl7.org/fhir/uv/genomics-reporting/StructureDefinition/obs-variant" />
      </type>
    </element>
    
    <!-- Constrain subject to be Indivdual -->
    <element id="DiagnosticReport.subject">
      <path value="DiagnosticReport.subject" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ga4gh.org/fhir/phenopackets/StructureDefinition/Individual" />
        <aggregation value="bundled" />
      </type>
    </element>
    
    <!-- Elements with fixed values -->
    <element id="DiagnosticReport.status">
      <path value="DiagnosticReport.status" />
      <fixedCode value="final" />
    </element>
    
    <!-- Forbidden elements -->
    <element id="DiagnosticReport.basedOn">
      <path value="DiagnosticReport.basedOn" />
      <max value="0" />
    </element>
    <element id="DiagnosticReport.category">
      <path value="DiagnosticReport.category" />
      <max value="0" />
    </element>
    <element id="DiagnosticReport.encounter">
      <path value="DiagnosticReport.encounter" />
      <max value="0" />
    </element>
    <element id="DiagnosticReport.effective[x]">
      <path value="DiagnosticReport.effective[x]" />
      <max value="0" />
    </element>
    <element id="DiagnosticReport.issued">
      <path value="DiagnosticReport.issued" />
      <max value="0" />
    </element>
    <element id="DiagnosticReport.performer">
      <path value="DiagnosticReport.performer" />
      <max value="0" />
    </element>
    <element id="DiagnosticReport.resultsInterpreter">
      <path value="DiagnosticReport.resultsInterpreter" />
      <max value="0" />
    </element>
    <element id="DiagnosticReport.imagingStudy">
      <path value="DiagnosticReport.imagingStudy" />
      <max value="0" />
    </element>
    <element id="DiagnosticReport.media">
      <path value="DiagnosticReport.media" />
      <max value="0" />
    </element>
    <element id="DiagnosticReport.conclusion">
      <path value="DiagnosticReport.conclusion" />
      <max value="0" />
    </element>
    <element id="DiagnosticReport.presentedForm">
      <path value="DiagnosticReport.presentedForm" />
      <max value="0" />
    </element>
  </differential>
</StructureDefinition>